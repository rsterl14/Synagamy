    {
      "id": "iui_partner",
      "title": "IUI With Partner Sperm",
      "description": "Intrauterine insemination using sperm from your partner",
      "recommended_for": "Couples with unexplained infertility, mild male factor, or ovulation issues",
      "steps": [
        {
          "step": "Initial Consultation & Testing",
          "overview": "Comprehensive fertility evaluation for both partners",
          "topic_refs": [
            "Referral to a Fertility Center (Male)",
            "Referral to a Fertility Center (Female)",
            "Semen Analysis",
            "Ovarian Reserve Testing"
          ]
        },
        {
          "step": "Screening & Baseline",
          "overview": "Early-cycle ultrasound and labs to confirm readiness",
          "topic_refs": [
            "Infectious Disease Screening (Male)",
            "Infectious Disease Screening (Female)"
          ]
        },
        {
          "step": "Ovulation Induction",
          "overview": "Decide on natural monitoring vs. medication to stimulate ovulation",
          "topic_refs": [
            "Ovulation Induction (Letrozole/Clomiphene)"
          ]
        },
        {
          "step": "Cycle Monitoring",
          "overview": "Track follicle growth with ultrasound and hormone levels",
          "topic_refs": [
            "Luteinizing Hormone (LH) Test",
            "Estradiol (E2) Blood Test"
          ]
        },
        {
          "step": "Trigger Shot",
          "overview": "Time insemination using hCG trigger or natural LH surge",
          "topic_refs": [
            "Human Chorionic Gonadotropin (hCG) Trigger"
          ]
        },
        {
          "step": "Sperm Collection & Preparation",
          "overview": "Collect and prepare sperm sample on day of IUI",
          "topic_refs": [
            "Sperm Collection",
            "Sperm Preparation (Density Gradient/Swim-Up)"
          ]
        },
        {
          "step": "IUI Procedure",
          "overview": "Place prepared sperm into uterine cavity",
          "topic_refs": [
            "Intrauterine Insemination (IUI)"
          ]
        },
        {
          "step": "Luteal Support",
          "overview": "Progesterone support if indicated",
          "topic_refs": [
            "Progesterone (Luteal Phase Support)"
          ]
        },
        {
          "step": "Pregnancy Test",
          "overview": "Blood test 2 weeks after IUI",
          "topic_refs": [
            "Serum hCG Pregnancy Test & Follow-up"
          ]
        }
      ]
    },
